{
    "name": "dfbag7/predis-async",
    "type": "library",
    "description": "Asynchronous version of Predis with no dependence on php-iredis and supported by Laravel v.4.2",
    "keywords": ["nosql", "redis", "predis"],
    "homepage": "http://github.com/dfbag7/predis-async",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniele Alessandri",
            "email": "suppakilla@gmail.com",
            "homepage": "http://clorophilla.net"
        },
        {
            "name": "Dmitry Bagdanov",
            "email": "dfbag7@mail.ru"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "predis/predis": "0.8.*",
        "react/event-loop": "~0.4",
        "clue/redis-protocol": "~0.3"
    },
    "autoload": {
        "psr-0": {"Predis\\Async": "lib/"}
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "minimum-stability": "dev"
}
